delimiter ////
create procedure cfinet213.sp_delete_urlproto_by_issecureidx(
	argAuditClusterId bigint,
	argAuditUserId varchar(36),
	argAuditSessionId varchar(36),
	secClusterId bigint,
	secTenantId bigint,
	argIsSecure boolean )
not deterministic
modifies sql data
begin
	declare curRevision int;
	declare subret boolean;
	declare curURLProtocolId integer;
	declare done int default false;
	declare cur_issecureidx cursor for
		select
			a109.urlprotocolid as urlprotocolid,
			a109.revision as revision
		from cfinet213.URLProto as a109
		where
			a109.issecure = argIsSecure;
	declare cur_issecureidx_normal cursor for
		select
			a109.urlprotocolid as urlprotocolid,
				a109.revision as revision
		from cfinet213.URLProto as a109
		where
				a109.issecure = argIsSecure;
	declare continue handler for not found set done = true;

	open cur_issecureidx_normal;
	read_loop_issecureidx_normal: loop
		fetch cur_issecureidx_normal into
				curURLProtocolId,
				curRevision;
		if done then
			leave read_loop_issecureidx_normal;
		end if;
			call cfinet213.sp_delete_urlproto( argAuditClusterId,
						argAuditUserId,
						argAuditSessionId,
						secClusterId,
						secTenantId,
						curURLProtocolId,
						curRevision );
	end loop;
	close cur_issecureidx_normal;
end;////
