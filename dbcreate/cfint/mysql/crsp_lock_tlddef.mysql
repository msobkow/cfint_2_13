delimiter ////

create procedure cfinet213.sp_lock_tlddef(
	argAuditClusterId bigint,
	argAuditUserId varchar(36),
	argAuditSessionId varchar(36),
	secClusterId bigint,
	secTenantId bigint,
	argTenantId bigint,
	argId bigint )
not deterministic
reads sql data
begin
	declare done boolean default false;
	declare continue handler for not found set done = true;
	select

			date_format( a106.createdat, '%Y-%m-%d %H:%i:%S' ) as createdat,
			a106.createdby as createdby,
			date_format( a106.updatedat, '%Y-%m-%d %H:%i:%S' ) as updatedat,
			a106.updatedby as updatedby,
			a106.tenantid as tenantid,
			a106.id as id,
			a106.safe_name as safe_name,
			a106.descr as descr,
			a106.revision as revision
	from cfinet213.tlddef as a106
	where
		a106.tenantid = argTenantId
		and a106.id = argId
	for update;
end;////
